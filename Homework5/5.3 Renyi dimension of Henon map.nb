Cell[BoxData[{
 RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], "\n", 
 RowBox[{
  RowBox[{"a", " ", "=", " ", "1.4"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"b", " ", "=", " ", "0.3"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"q", " ", "=", " ", "0"}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"data", " ", "=", " ", 
   RowBox[{"Drop", "[", 
    RowBox[{"NestList", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"#", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "+", "1", "-", 
          RowBox[{"a", "*", 
           RowBox[{
            RowBox[{"#", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "^", "2"}]}]}], ",", 
         RowBox[{"b", "*", 
          RowBox[{"#", "[", 
           RowBox[{"[", "1", "]"}], "]"}]}]}], "}"}], " ", "&"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"0.1", ",", "0.1"}], "}"}], ",", " ", 
      RowBox[{
       RowBox[{"2", "*", 
        RowBox[{"10", "^", "6"}]}], "-", "1"}]}], "]"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"bins", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Flatten", "[", 
      RowBox[{"BinCounts", "[", 
       RowBox[{"data", ",", " ", "\[Epsilon]", ",", " ", "\[Epsilon]"}], 
       "]"}], "]"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"\[Epsilon]", ",", "0.001", ",", "0.02", ",", "0.001"}], 
      "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"NBoxes", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"2", "*", 
       RowBox[{"10", "^", "6"}]}], "-", "1"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", 
       RowBox[{"Length", "[", "bins", "]"}]}], "}"}]}], "]"}]}], ";"}], "\n", 

 RowBox[{
  RowBox[{
   RowBox[{"probabilities", " ", "=", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Select", "[", 
       RowBox[{
        RowBox[{"Divide", "[", 
         RowBox[{
          RowBox[{"bins", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", 
          RowBox[{"NBoxes", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", 
        RowBox[{
         RowBox[{"#", ">", "0"}], "&"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", 
        RowBox[{"Length", "[", "bins", "]"}]}], "}"}]}], "]"}]}], ";"}], 
  "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"sumProb", " ", "=", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        SuperscriptBox["p", "q"], ",", 
        RowBox[{"{", 
         RowBox[{"p", ",", " ", 
          RowBox[{"probabilities", "[", 
           RowBox[{"[", "i", "]"}], "]"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", 
        RowBox[{"Length", "[", "bins", "]"}]}], "}"}]}], "]"}]}], ";"}], 
  "\n"}], "\n", 
 RowBox[{
  RowBox[{"yValues", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Divide", "[", 
      RowBox[{
       RowBox[{"Log", "[", 
        RowBox[{"sumProb", "[", 
