Cell[BoxData[{
 RowBox[{"ClearAll", "[", "\"\<Global`*\>\"", "]"}], "\n", 
 RowBox[{
  RowBox[{"a", " ", "=", " ", "1.4"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"b", " ", "=", " ", "0.3"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"IterationSteps", " ", "=", " ", 
   RowBox[{
    RowBox[{"2", "*", 
     RowBox[{"10", "^", "6"}]}], "-", "1"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"DQ", " ", "=", " ", 
   RowBox[{"ParallelTable", "[", "\n", "  ", 
    RowBox[{
     RowBox[{
      RowBox[{"data", " ", "=", " ", 
       RowBox[{"Drop", "[", 
        RowBox[{"NestList", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"#", "[", 
               RowBox[{"[", "2", "]"}], "]"}], " ", "+", " ", "1", " ", "-", 
              " ", 
              RowBox[{"a", "*", 
               RowBox[{
                RowBox[{"#", "[", 
                 RowBox[{"[", "1", "]"}], "]"}], "^", "2"}]}]}], ",", " ", 
             RowBox[{"b", "*", 
              RowBox[{"#", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}]}], "}"}], " ", "&"}], ",", 
          " ", 
          RowBox[{"{", 
           RowBox[{"0.1", ",", " ", "0.1"}], "}"}], ",", " ", 
          "IterationSteps"}], "]"}], "]"}]}], ";", "\n", "  ", 
      RowBox[{"bins", " ", "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"BinCounts", "[", 
           RowBox[{"data", ",", " ", "\[Epsilon]", ",", " ", "\[Epsilon]"}], 
           "]"}], "]"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{
          "\[Epsilon]", ",", " ", "0.001", ",", " ", "0.02", ",", " ", 
           "0.001"}], "}"}]}], "]"}]}], ";", "\n", "  ", 
      RowBox[{"NBoxes", " ", "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{"IterationSteps", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"Length", "[", "bins", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\n", "  ", 
      RowBox[{"probabilities", " ", "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Select", "[", 
          RowBox[{
           RowBox[{"Divide", "[", 
            RowBox[{
             RowBox[{"bins", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", " ", 
             RowBox[{"NBoxes", "[", 
              RowBox[{"[", "i", "]"}], "]"}]}], "]"}], ",", " ", 
           RowBox[{
            RowBox[{"#", " ", ">", " ", "0"}], " ", "&"}]}], "]"}], ",", " ", 
         
         RowBox[{"{", 
          RowBox[{"i", ",", " ", 
           RowBox[{"Length", "[", "bins", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\n", "  ", "\n", "  ", 
      RowBox[{"sumProb", " ", "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{"p", "^", "q"}], ",", " ", 
           RowBox[{"{", 
            RowBox[{"p", ",", " ", 
             RowBox[{"probabilities", "[", 
              RowBox[{"[", "i", "]"}], "]"}]}], "}"}]}], "]"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i", ",", " ", 
           RowBox[{"Length", "[", "bins", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\n", "\n", "  ", 
      RowBox[{"yValues", " ", "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Divide", "[", 
          RowBox[{
           RowBox[{"Log", "[", 
            RowBox[{"sumProb", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", " ", 
           RowBox[{"(", 
            RowBox[{"1", " ", "-", " ", "q"}], ")"}]}], "]"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i", ",", " ", 
           RowBox[{"Length", "[", "bins", "]"}]}], "}"}]}], "]"}]}], ";", 
      "\n", "  ", 
      RowBox[{"xValues", " ", "=", " ", 
       RowBox[{"Log", "[", 
        RowBox[{"1", "/", 
         RowBox[{"Range", "[", 
          RowBox[{"0.001", ",", "0.02", ",", "0.001"}], "]"}]}], "]"}]}], ";",
       "\n", "  ", "\n", "  ", 
      RowBox[{"nominator", " ", "=", " ", 
       RowBox[{
        RowBox[{"Last", "[", "yValues", "]"}], " ", "-", " ", 
        RowBox[{"First", "[", "yValues", "]"}]}]}], ";", "\n", "  ", 
      RowBox[{"denominator", " ", "=", " ", 
       RowBox[{
        RowBox[{"Last", "[", "xValues", "]"}], " ", "-", " ", 
        RowBox[{"First", "[", "xValues", "]"}]}]}], ";", "\n", "  ", 
      RowBox[{"slope", " ", "=", " ", 
       RowBox[{"nominator", "/", "denominator"}]}], ";", "\n", "  ", 
